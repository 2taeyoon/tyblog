{
  "rewrites": [
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/favicon/(.*)", "destination": "/favicon/$1" },
		{ "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
	"headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ]
}