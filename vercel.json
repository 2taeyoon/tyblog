{
  "rewrites": [
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/favicon/(.*)", "destination": "/favicon/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
	"headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ]
}